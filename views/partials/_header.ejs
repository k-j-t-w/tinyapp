<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <% if (!user_id) { %>
      <form class="form-inline" action="/registerred" method="POST">
        <div class="form-group mb-2">
          <button type="submit" style="margin-right: 20px" class="btn btn-outline-dark" >Register</button>
          
        </div>
      </form>
      <form class="form-inline" action="/login" method="POST">
        <div class="form-group mb-2">
          <button type="submit" class="btn btn-outline-dark" >Login</button>
        </div>
      </form>
    <% } else { %>
      <p>Logged in as: <%= users[user_id].email %>!</p>
      <form class="form-inline" action="/logout" method="POST" style="margin-left: 10px" style="margin-right: 10px">
        <button type="submit" class="btn btn-outline-dark">Logout</button>
      </form>
    <% } %>
  </nav>
</header>